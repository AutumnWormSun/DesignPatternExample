qmake_all: FORCE

dist: 
	@test -d .tmp/state1.0.0 || mkdir -p .tmp/state1.0.0
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/state1.0.0/ && (cd `dirname .tmp/state1.0.0` && $(TAR) state1.0.0.tar state1.0.0 && $(COMPRESS) state1.0.0.tar) && $(MOVE) `dirname .tmp/state1.0.0`/state1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/state1.0.0


clean:compiler_clean 
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


